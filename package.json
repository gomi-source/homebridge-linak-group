  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/bent": "^7.3.8",
    "@types/node": "^24.5.2",
    "eslint": "^9.36.0",
    "homebridge": "^2.0.0-beta.30",
    "nodemon": "^3.1.10",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.0"
  }
